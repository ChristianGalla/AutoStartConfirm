<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoStartConfirm"
        xmlns:AutoStarts="clr-namespace:AutoStartConfirm.AutoStarts" x:Name="window" x:Class="AutoStartConfirm.MainWindow"
        mc:Ignorable="d"
        Title="Auto start confirm" Height="408.667" Width="913.667">
    <Grid x:Name="grid">
        <TabControl x:Name="tabControl" Padding="5" Margin="0">
            <TabItem Header="Current" VerticalAlignment="Top">
                <DataGrid x:Name="CurrentAutoStartGrid" MinHeight="50" MinWidth="200" >
                </DataGrid>
            </TabItem>
            <TabItem Header="TabItem">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>

    </Grid>
</Window>
